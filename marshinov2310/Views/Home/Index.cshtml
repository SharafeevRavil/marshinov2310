@model IEnumerable<marshinov2310.Models.Product>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @{
        foreach (var product in Model)
        {
            <p>@product.Name</p>
            <p>@product.price</p>
            <p>@product.Description</p>
            <br/>
            <br/>
        }
    }
</div>